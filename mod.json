{
	"geode": "4.7.0",
	"version": "v1.7.1",
	"gd": {
        "win": "2.2074",
        "android": "2.2074",
        "mac": "2.2074",
        "ios": "2.2074"
    },
	"id": "cvolton.misc_bugfixes",
	"name": "Misc Bugfixes",
	"developer": "Cvolton",
	"description": "The 2.2075 before actual 2.2075...",
    "tags": ["bugfix"],
	"links": {
		"source": "https://github.com/Cvolton/miscbugfixes-geode"
	},
    "early-load": true,
	"settings": {
        "practice-mode-title": {
            "name": "Gameplay",
            "type": "title"
        },
		"practice-mode-show-hitboxes": {
            "name": "Fix \"Show Hitboxes\" changing physics",
            "type": "bool",
			"default": true
        },
		"other-wave-trail-fix": {
            "name": "Fix Player 2 Wave Trail visual bug",
            "type": "bool",
			"default": true
        },
		"enforce-start-pos-mirror-mode": {
            "name": "Fix Start Pos Mirror Mode being ignored",
            "type": "bool",
			"default": true
        },
		"fix-mini-swing": {
            "name": "Fix Mini Swing showing Cube instead with Default Icon enabled",
            "type": "bool",
			"default": true
        },
        "show-trigger-wave-particles-fix": {
            "name": "Fix Wave particles after Show trigger",
            "description": "Fix Wave showing Ship/UFO fire particles after hiding and showing the icon with Hide/Show triggers",
            "type": "bool",
            "default": true
        },
        "practice-music-sync-pulse-fix": {
            "name": "Fix pulsing objects with Practice Music Sync",
            "type": "bool",
            "default": true
        },
        "dash-particles-offset-fix": {
            "name": "Fix dash orb particles being incorrectly offset",
            "type": "bool",
            "default": true
        },
        "other-title": {
            "name": "Other",
            "type": "title"
        },
        "skip-ccleaner": {
            "name": "Skip File Cleanup",
            "type": "bool",
            "default": false
        }
	},
	"api": {
		"include": [
			"src/_Utils.hpp"
		]
	}
}
